<h1>Create a Bench </h1>

<%= form_for @bench do |f| %>

  <%= f.select :location_id, @locations.map{|l| [l.name, l.id]} %>
  <div>
  <%= f.label :description %>
  <%= f.text_area :description %>
  </div>
  <div>
  <%= f.label :image %>
  <%= f.text_field :image %>
  </div>
  <%= f.submit %>
<% end %>